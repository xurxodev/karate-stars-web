    "hapi-auth-jwt2": "^10.2.0",
    "instatouch": "^2.3.14",
    "joi": "^17.6.0",
    "karate-stars-core": "0.1.0",
    "mime-types": "^2.1.34",
    "moment": "^2.29.1",
    "mongodb": "^4.3.1",
    "node-fetch": "^3.2.0",
    "rss-parser": "^3.12.0",
    "twit": "^2.2.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/hapi__hapi": "^20.0.10",
