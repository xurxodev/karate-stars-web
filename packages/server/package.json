{
    "name": "karate-stars-server",
    "version": "0.1.0",
    "description": "Karate stars Server. Developed using TypeScript in Node.js and using hapijs framework.",
    "main": "build/index.js",
    "repository": "https://github.com/xurxodev/karate-stars-api",
    "author": "Jorge SaÃÅnchez <xurxosf@gmail.com>",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "import-news": "yarn build && node -r dotenv/config ./build/news-import/importNews.js",
        "import-ranking": "yarn build && node -r dotenv/config ./build/ranking-import/importRanking.js",
        "start": "node ./build/index.js",
        "build": "yarn clean && yarn tsc",
        "start-dev": "../../node_modules/nodemon/bin/nodemon.js -e ts  --exec \"yarn build-dev\"",
        "build-dev": "yarn build && node -r dotenv/config ./build/index.js",
        "start-debug": "yarn build && node -r dotenv/config --inspect ./build/index.js",
        "tsc": "tsc --build",
        "clean": "rm -rf build",
        "test": "jest",
        "migrate:create": "sh -c 'ts-node  -r dotenv/config ../../node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.js' ",
        "migrate:status": "ts-node  -r dotenv/config ../../node_modules/.bin/migrate-mongo status -f migrate-mongo-config.js",
        "migrate:undo-last": "ts-node  -r dotenv/config ../../node_modules/.bin/migrate-mongo down -f migrate-mongo-config.js",
        "migrate": "ts-node -r dotenv/config ../../node_modules/.bin/migrate-mongo up -f migrate-mongo-config.js"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.4",
        "@hapi/hapi": "^20.2.1",
        "@hapi/inert": "^6.0.5",
        "axios": "^0.25.0",
        "dotenv": "^14.3.2",
        "firebase-admin": "^10.0.2",
        "hapi-auth-jwt2": "^10.2.0",
        "instatouch": "^2.3.14",
        "joi": "^17.6.0",
        "karate-stars-core": "0.1.0",
        "mime-types": "^2.1.34",
        "moment": "^2.29.2",
        "mongodb": "^4.3.1",
        "rss-parser": "^3.12.0",
        "twit": "^2.2.11",
        "uuid": "^8.3.2",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@types/hapi__hapi": "^20.0.10",
        "@types/hapi__inert": "^5.2.3",
        "@types/jest": "^26.0.20",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/mime-types": "^2.1.1",
        "@types/node": "^17.0.13",
        "@types/supertest": "^2.0.11",
        "@types/twit": "^2.2.30",
        "@types/uuid": "^8.3.4",
        "@types/xml2js": "^0.4.9",
        "jest": "^26.6.0",
        "migrate-mongo": "^8.2.3",
        "nodemon": "^2.0.15",
        "supertest": "^6.2.2",
        "ts-jest": "^26.5.3",
        "ts-node": "10.4.0"
    },
    "engines": {
        "node": "14.x"
    }
}
